<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="vBoxMain" alignment="CENTER" prefHeight="217.0" prefWidth="410.0" spacing="10.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <Label id="lblChannelName" prefWidth="160.0" text="Channel Name:" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets top="2.0" />
               </HBox.margin>
            </Label>
            <TextField id="tfChannelName" prefWidth="200.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox id="hBoxChannelType" VBox.vgrow="ALWAYS">
         <children>
            <HBox prefHeight="20.0" prefWidth="162.0" HBox.hgrow="ALWAYS">
               <children>
                  <RadioButton id="radioBtnText" mnemonicParsing="false" prefHeight="20.0" prefWidth="166.0" text="Text" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </RadioButton>
               </children>
            </HBox>
            <HBox prefHeight="20.0" prefWidth="208.0" HBox.hgrow="ALWAYS">
               <children>
                  <RadioButton id="radioBtnAudio" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Audio" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </RadioButton>
               </children>
            </HBox>
         </children>
      </HBox>
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <Label id="lblPrivileged" prefWidth="140.0" text="Privileged:" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Label>
            <CheckBox id="checkBoxPrivileged" mnemonicParsing="false" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </CheckBox>
         </children>
      </HBox>
      <HBox id="hBoxLblMembers" VBox.vgrow="ALWAYS">
         <children>
            <Label id="lblMembers" text="Members:" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox id="vBoxMemberNameAndCheckBox" alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <fx:include source="subview/MemberListSubView.fxml" />
         </children>
      </VBox>
      <Label id="lblError" textFill="RED" VBox.vgrow="ALWAYS" />
      <HBox prefHeight="0.0" prefWidth="370.0" VBox.vgrow="ALWAYS">
         <children>
            <Button id="btnDeleteChannel" defaultButton="true" mnemonicParsing="false" prefWidth="150.0" styleClass="styleButton" text="Delete" textFill="RED" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets right="70.0" />
               </HBox.margin>
            </Button>
            <Button id="btnSave" defaultButton="true" mnemonicParsing="false" prefWidth="150.0" styleClass="styleButton" text="Save" HBox.hgrow="ALWAYS">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <styleClass>
      <String fx:value="styleBorder" />
      <String fx:value="styleBackgroundHighlight" />
   </styleClass>
</VBox>
