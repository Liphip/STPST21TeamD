<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox id="mainVBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0">
          <children>
              <HBox alignment="CENTER_LEFT" spacing="100.0" styleClass="styleBackgroundPopup">
                  <children>
                      <Label id="lblDarkMode" text="%DARKMODE">
                          <font>
                              <Font size="14.0" />
                          </font>
                      </Label>
                      <CheckBox id="btnDarkMode" mnemonicParsing="false" />
                  </children>
              </HBox>
              <HBox id="hBoxLanguage" alignment="CENTER_LEFT" spacing="91.0">
                  <children>
                      <Label id="lblLanguage" text="%LANGUAGE">
                          <font>
                              <Font size="14.0" />
                          </font>
                      </Label>
                      <ChoiceBox id="choiceBoxLanguage" />
                  </children>
                  <VBox.margin>
                      <Insets />
                  </VBox.margin>
              </HBox>
            <VBox id="vBoxExtraSettings" spacing="20.0">
               <children>
                    <HBox alignment="CENTER" spacing="25.0">
                        <Label id="lbTextSize" text="%CHAT_TEXT_SIZE">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Label>
                        <Slider id="sliderTextSize" blockIncrement="0.0" majorTickUnit="2.0" max="20.0" min="12.0" minorTickCount="0" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="12.0" />
                    </HBox>
                  <HBox>
                     <children>
                        <Label id="lbConnections" text="%CONNECTIONS">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="20.0">
                     <children>
                        <Button id="btnSpotify" mnemonicParsing="false">
                           <graphic>
                              <ImageView fitHeight="50.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@images/spotify.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button id="btnSteam" mnemonicParsing="false">
                           <graphic>
                              <ImageView fitHeight="50.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@images/steam.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </HBox>
                    <HBox id="hBoxBtns" alignment="CENTER_LEFT" spacing="115.0">
                     <Label id="lbLogout" text="%LOGOUT">
                        <font>
                           <Font size="14.0" />
                        </font>
                     </Label>
                        <Button id="btnLogout" mnemonicParsing="false" styleClass="logoutButton">
                            <graphic>
                                <ImageView id="btnLogOut" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                    <Image url="@images/log_out.png" />
                                </ImageView>
                            </graphic>
                        <tooltip>
                           <Tooltip text="%LOGOUT" />
                        </tooltip>
                        </Button>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                    </HBox>
               </children>
            </VBox>
          </children>
          <styleClass>
              <String fx:value="styleBackgroundPopup" />
              <String fx:value="styleBorder" />
          </styleClass>
          <padding>
              <Insets bottom="5.0" left="5.0" right="5.0" />
          </padding>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </VBox>
      <VBox id="vBoxSoundSettings" spacing="20.0">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="30.0">
               <children>
                  <Label minWidth="120.0" text="%OUTPUT_DEVICE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="choiceBoxOutputDevice" minWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT">
               <children>
                  <ImageView fitHeight="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/speaker-with-no-sound.png" />
                     </image>
                  </ImageView>
                  <Slider id="sliderOutputVolume" blockIncrement="5.0" value="50.0" />
                  <ImageView fitHeight="20.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/speaker-with-sound.png" />
                     </image>
                  </ImageView>
               </children>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" spacing="30.0">
               <children>
                  <Label minWidth="120.0" text="%INPUT_DEVICE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <ChoiceBox id="choiceBoxInputDevice" minWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT">
               <children>
                  <ImageView fitHeight="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/speaker-with-no-sound.png" />
                     </image>
                  </ImageView>
                  <Slider id="sliderInputVolume" blockIncrement="5.0" value="50.0" />
                  <ImageView fitHeight="20.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/speaker-with-sound.png" />
                     </image>
                  </ImageView>
               </children>
               <padding>
                  <Insets right="10.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" spacing="30.0">
               <children>
                  <Label text="%INPUT_SENSITIVITY">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER">
               <children>
                  <Slider id="sliderInputSensitivity" blockIncrement="5.0" value="50.0" />
               </children>
            </HBox>
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button id="btnTestSetup" alignment="CENTER" mnemonicParsing="false" text="%TEST_SETUP" />
                  <ProgressBar id="prgBarSetupTest" prefWidth="200.0" progress="0.0" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets left="40.0" />
         </padding>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <styleClass>
      <String fx:value="styleBackgroundPopup" />
      <String fx:value="styleBorder" />
   </styleClass>
</HBox>
