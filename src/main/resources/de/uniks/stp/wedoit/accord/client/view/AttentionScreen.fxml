<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="150.0" prefWidth="335.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Region prefHeight="15.0" prefWidth="350.0"/>
        <Label id="lblAttention" prefHeight="30.0" styleClass="redLabel" text="ATTENTION">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <Label id="lblError" prefHeight="20.0" styleClass="redLabel" text="Something went wrong!"
               textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <HBox alignment="CENTER" prefHeight="20.0" prefWidth="200.0">
            <children>
                <Label id="lblAreYouSure" text="Are you sure you want to delete the ">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <Label id="lblObjectToDelete" text="&lt;Object&gt;">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
                <Label text="?">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <Region prefHeight="20.0" prefWidth="350.0"/>
        <HBox alignment="CENTER" prefHeight="60.0" prefWidth="350.0">
            <children>
                <Button id="btnDiscard" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0"
                        styleClass="styleButton" text="Discard">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Region prefHeight="100.0" prefWidth="102.0"/>
                <Button id="btnDelete" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0"
                        styleClass="styleButton" text="Delete">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
            </children>
        </HBox>
    </children>
    <styleClass>
        <String fx:value="styleBorder"/>
        <String fx:value="styleBackgroundHighlight"/>
    </styleClass>
</VBox>
