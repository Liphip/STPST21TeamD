<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="5.0" xmlns="http://javafx.com/javafx">
    <children>
        <Label id="lblAttention" alignment="CENTER" contentDisplay="CENTER" styleClass="redLabel" text="%ATTENTION">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <Label id="lblError" alignment="CENTER" contentDisplay="CENTER" styleClass="redLabel" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <HBox alignment="CENTER">
            <children>
                <Label id="lblAreYouSure" alignment="CENTER" contentDisplay="CENTER">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER">
            <children>
                <Button id="btnDiscard" mnemonicParsing="false" styleClass="styleButton" text="%DISCARD">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Region minWidth="102.0" prefWidth="102.0"/>
                <Button id="btnDelete" mnemonicParsing="false" styleClass="styleButton" text="%DELETE">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
            </children>
        </HBox>
    </children>
    <styleClass>
        <String fx:value="styleBorder"/>
        <String fx:value="styleBackgroundPopup"/>
    </styleClass>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
