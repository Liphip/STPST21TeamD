<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<HBox id="hBoxOuter" spacing="20.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox id="vBoxButtons" alignment="TOP_CENTER" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" spacing="20.0" styleClass="styleBackgroundHighlight">
         <children>
            <Button id="btnBack" mnemonicParsing="false" styleClass="sidebar-Button" text="BACK">
               <tooltip>
                  <Tooltip text="BACK" />
               </tooltip>
            </Button>
            <Button id="btnAppearance" mnemonicParsing="false" styleClass="sidebar-Button" text="Appearance" />
            <Button id="btnLanguage" mnemonicParsing="false" styleClass="sidebar-Button" text="Language" />
            <Button id="btnConnections" mnemonicParsing="false" styleClass="sidebar-Button" text="Connections" />
            <Button id="btnVoice" mnemonicParsing="false" styleClass="sidebar-Button" text="Voice" />
            <Button id="btnDescription" mnemonicParsing="false" styleClass="sidebar-Button" text="Description" />
            <Region VBox.vgrow="ALWAYS" />
            <Button id="btnLogout" mnemonicParsing="false" styleClass="sidebar-Button" text="%LOGOUT">
               <tooltip>
                  <Tooltip text="%LOGOUT" />
               </tooltip>
            </Button>
         </children>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </VBox>
   </children>
</HBox>
